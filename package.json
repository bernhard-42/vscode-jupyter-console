{
  "name": "vscode-jupyter-console",
  "displayName": "Jupyter Console",
  "description": "Run Python code with Jupyter kernel and jupyter-console integration",
  "version": "1.2.5",
  "publisher": "bernhard-42",
  "license": "MIT",
  "repository": "https://github.com/bernhard-42/vscode-jupyter-console",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "keywords": [
    "jupyter",
    "console",
    "python",
    "interactive"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Jupyter Console",
      "properties": {
        "jupyterConsole.autoStartKernel": {
          "type": "boolean",
          "default": true,
          "description": "Automatically start kernel when executing code without a running kernel. If disabled, you will be prompted to start the kernel.",
          "order": 0
        },
        "jupyterConsole.executionBlockPrefix": {
          "type": "string",
          "default": "Out[{filename}: {counter}]:",
          "description": "Prefix marker shown before code execution in Jupyter Console, {filename} and {counter} are placeholders and will be set by the extension. Set to empty string to hide.",
          "order": 1
        },
        "jupyterConsole.executionBlockSuffixSuccess": {
          "type": "string",
          "default": "✔︎",
          "description": "Suffix marker shown after successful code execution in Jupyter Console. Displayed in green. Set to empty string to hide.",
          "order": 2
        },
        "jupyterConsole.executionBlockSuffixFailure": {
          "type": "string",
          "default": "✘",
          "description": "Suffix marker shown after failed code execution in Jupyter Console. Displayed in red. Set to empty string to hide.",
          "order": 3
        },
        "jupyterConsole.executionBlockEnforcePrompt": {
          "type": "boolean",
          "default": false,
          "description": "Enforce jupyter console.to show the In[...] prompt. Due to jupyter console limitations, it will appear twice",
          "order": 4
        },
        "jupyterConsole.enableOutputViewer": {
          "type": "boolean",
          "default": false,
          "description": "Enable the Jupyter Output viewer terminal. Disable to use only the Jupyter Console terminal.",
          "order": 5
        },
        "jupyterConsole.truncateInputLinesMax": {
          "type": "number",
          "default": 10,
          "description": "Maximum number of input code lines to display before truncating in the Jupyter Output terminal. Set to 0 to show all lines.",
          "minimum": 0,
          "order": 6
        },
        "jupyterConsole.advanced.viewerTerminalStartDelay": {
          "type": "number",
          "default": 1000,
          "description": "[Advanced] Delay (in milliseconds) before starting the output viewer terminal. Increase on slower machines.",
          "minimum": 0,
          "order": 10
        },
        "jupyterConsole.advanced.consoleTerminalStartDelay": {
          "type": "number",
          "default": 1000,
          "description": "[Advanced] Delay (in milliseconds) before starting the console terminal. Allows time for conda/virtualenv activation to complete. Increase if activation commands appear in Jupyter Console.",
          "minimum": 0,
          "order": 11
        },
        "jupyterConsole.advanced.kernelOperationWait": {
          "type": "number",
          "default": 1000,
          "description": "[Advanced] Wait time (in milliseconds) for kernel operations like restart. Increase on slower machines.",
          "minimum": 0,
          "order": 12
        },
        "jupyterConsole.advanced.kernelConnectionTimeout": {
          "type": "number",
          "default": 10000,
          "description": "[Advanced] Timeout (in milliseconds) for kernel connection file to appear. Increase on slower machines.",
          "minimum": 1000,
          "order": 13
        },
        "jupyterConsole.advanced.interruptTimeout": {
          "type": "number",
          "default": 3000,
          "description": "[Advanced] Timeout (in milliseconds) before force-killing an unresponsive kernel after interrupt. Useful for native code that doesn't respond to interrupts.",
          "minimum": 1000,
          "order": 14
        },
        "jupyterConsole.advanced.consoleIsCompleteTimeout": {
          "type": "number",
          "default": 3600,
          "description": "[Advanced] Timeout (in seconds) for Jupyter Console to wait for kernel's is_complete response. If kernel is busy longer than this, multi-line editing may require multiple Enter presses. Increase for long-running computations.",
          "minimum": 1,
          "order": 15
        }
      }
    },
    "commands": [
      {
        "command": "jupyterConsole.startKernel",
        "title": "Jupyter Console: Start Kernel",
        "icon": "$(play)"
      },
      {
        "command": "jupyterConsole.stopKernel",
        "title": "Jupyter Console: Stop Kernel",
        "icon": "$(debug-stop)"
      },
      {
        "command": "jupyterConsole.restartKernel",
        "title": "Jupyter Console: Restart Kernel",
        "icon": "$(debug-restart)"
      },
      {
        "command": "jupyterConsole.interruptKernel",
        "title": "Jupyter Console: Interrupt Kernel",
        "icon": "$(debug-pause)"
      },
      {
        "command": "jupyterConsole.startConsole",
        "title": "Jupyter Console: Start Console Terminal",
        "icon": "$(terminal)"
      },
      {
        "command": "jupyterConsole.runCell",
        "title": "Jupyter Console: Run Cell"
      },
      {
        "command": "jupyterConsole.runCellAndAdvance",
        "title": "Jupyter Console: Run Cell and Advance"
      },
      {
        "command": "jupyterConsole.runSelection",
        "title": "Jupyter Console: Run Selection/Line"
      },
      {
        "command": "jupyterConsole.runSelectionAndAdvance",
        "title": "Jupyter Console: Run Selection/Line and Advance"
      },
      {
        "command": "jupyterConsole.runAll",
        "title": "Jupyter Console: Run All"
      }
    ],
    "submenus": [
      {
        "id": "jupyterConsole.kernelMenu",
        "label": "Jupyter Kernel",
        "icon": "$(notebook-kernel-select)"
      },
      {
        "id": "jupyterConsole.runMenu",
        "label": "Run in Console",
        "icon": "$(run-all)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "submenu": "jupyterConsole.kernelMenu",
          "when": "editorLangId == python",
          "group": "navigation"
        },
        {
          "submenu": "jupyterConsole.runMenu",
          "when": "editorLangId == python",
          "group": "navigation"
        }
      ],
      "jupyterConsole.kernelMenu": [
        {
          "command": "jupyterConsole.startKernel",
          "group": "1_kernel@1"
        },
        {
          "command": "jupyterConsole.stopKernel",
          "group": "1_kernel@2"
        },
        {
          "command": "jupyterConsole.restartKernel",
          "group": "1_kernel@3"
        }
      ],
      "jupyterConsole.runMenu": [
        {
          "command": "jupyterConsole.startConsole",
          "group": "1_console@1"
        },
        {
          "command": "jupyterConsole.runCellAndAdvance",
          "group": "2_cell@1"
        },
        {
          "command": "jupyterConsole.runCell",
          "group": "2_cell@2"
        },
        {
          "command": "jupyterConsole.runSelectionAndAdvance",
          "group": "3_run@1"
        },
        {
          "command": "jupyterConsole.runSelection",
          "group": "3_run@2"
        },
        {
          "command": "jupyterConsole.runAll",
          "group": "4_all@1"
        },
        {
          "command": "jupyterConsole.interruptKernel",
          "group": "5_interrupt@1"
        }
      ]
    },
    "keybindings": [
      {
        "command": "jupyterConsole.runCellAndAdvance",
        "key": "ctrl+enter",
        "when": "editorTextFocus && editorLangId == python"
      },
      {
        "command": "jupyterConsole.runCell",
        "key": "shift+ctrl+enter",
        "when": "editorTextFocus && editorLangId == python"
      },
      {
        "command": "jupyterConsole.runSelectionAndAdvance",
        "key": "alt+enter",
        "when": "editorTextFocus && editorLangId == python"
      },
      {
        "command": "jupyterConsole.runSelection",
        "key": "shift+alt+enter",
        "when": "editorTextFocus && editorLangId == python"
      },
      {
        "command": "jupyterConsole.interruptKernel",
        "key": "ctrl+alt+i",
        "when": "editorTextFocus && editorLangId == python"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn compile",
    "compile": "tsc -p ./ && yarn copy-python",
    "copy-python": "node -e \"require('fs').copyFileSync('src/iopub_viewer.py', 'out/iopub_viewer.py'); require('fs').copyFileSync('src/kernel_manager.py', 'out/kernel_manager.py');\"",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn compile && yarn lint",
    "test": "node ./out/test/runTest.js",
    "test:unit": "TEST_FILTER=unit node ./out/test/runTest.js",
    "test:integration": "TEST_FILTER=integration node ./out/test/runTest.js",
    "test:kernel": "TEST_FILTER=kernelClient node ./out/test/runTest.js",
    "test:executor": "TEST_FILTER=codeExecutor node ./out/test/runTest.js",
    "test:console": "TEST_FILTER=consoleManager node ./out/test/runTest.js",
    "lint": "eslint src --ext ts",
    "package": "vsce package --yarn",
    "package:win32": "vsce package --yarn --target win32-x64",
    "package:linux": "vsce package --yarn --target linux-x64",
    "package:mac-intel": "vsce package --yarn --target darwin-x64",
    "package:mac-arm": "vsce package --yarn --target darwin-arm64",
    "release": "VERSION=$(node -p \"require('./package.json').version\") && git add . && (git diff --cached --quiet || git commit -m \"building release $VERSION\") && git tag \"v$VERSION\""
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "18.x",
    "@types/sinon": "^17.0.4",
    "@types/uuid": "^11.0.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.2.1",
    "chai": "^6.2.0",
    "eslint": "^8.54.0",
    "mocha": "^11.7.5",
    "sinon": "^21.0.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "uuid": "^13.0.0",
    "zeromq": "^6.5.0"
  }
}